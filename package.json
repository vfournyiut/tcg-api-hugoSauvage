{
  "name": "template-tcg-api",
  "version": "1.0.0",
  "author": "",
  "description": "TCG Pokemon-like Backend Server",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "ts:check": "tsc --noEmit",
    "start": "node dist/index.js",
    "test": "vitest --coverage --run",
    "test:ui": "vitest --ui",
    "db:start": "docker compose up -d",
    "db:stop": "docker compose down",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "npm run db:generate && tsx prisma/seed.ts",
    "db:reset": "npm run db:generate && prisma migrate reset --force && npm run db:seed"
  },
  "dependencies": {
    "@prisma/adapter-pg": "7.1.0",
    "@prisma/client": "7.1.0",
    "bcrypt": "^5.1.1",
    "bcryptjs": "2.4.3",
    "cors": "2.8.5",
    "dotenv": "16.3.1",
    "express": "4.18.2",
    "http-status-codes": "2.3.0",
    "js-yaml": "4.1.1",
    "jsonwebtoken": "9.0.2",
    "pg": "8.16.3",
    "prisma": "7.1.0",
    "socket.io": "^4.6.1",
    "socket.io-client": "^4.8.3",
    "swagger-ui-express": "^5.0.1",
    "uuid": "13.0.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "2.4.6",
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/js-yaml": "4.0.9",
    "@types/jsonwebtoken": "9.0.5",
    "@types/node": "20.19.25",
    "@types/pg": "8.16.0",
    "@types/socket.io": "^3.0.1",
    "@types/supertest": "^6.0.3",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "10.0.0",
    "@types/yamljs": "^0.2.34",
    "@vitest/coverage-v8": "^4.0.15",
    "@vitest/ui": "4.0.15",
    "supertest": "^7.2.2",
    "tsx": "4.7.0",
    "typescript": "5.4.5",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "^4.0.15",
    "vitest-mock-extended": "3.1.0"
  }
}
